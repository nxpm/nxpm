{
  "version": 1,
  "projects": {
    "docs": {
      "projectType": "application",
      "root": "apps/docs",
      "sourceRoot": "apps/docs/src",
      "architect": {
        "serve": {
          "builder": "@nx-plus/docusaurus:docusaurus",
          "options": {
            "port": 4280
          }
        },
        "build": {
          "builder": "@nx-plus/docusaurus:build-docusaurus",
          "options": {
            "outputPath": "dist/docs"
          }
        },
        "publish": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn nx build docs"
              },
              {
                "command": "cd dist/docs && git init && git add . && git commit -m 'feat: publish site' && git remote add origin git@github.com:nxpm/nxpm.github.io.git && git push --set-upstream origin master --force"
              }
            ],
            "parallel": false
          }
        }
      }
    },
    "schematics": {
      "root": "libs/schematics",
      "sourceRoot": "libs/schematics/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/schematics/tsconfig.lib.json", "libs/schematics/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/schematics/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/schematics/jest.config.js",
            "tsConfig": "libs/schematics/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/schematics",
            "tsConfig": "libs/schematics/tsconfig.lib.json",
            "packageJson": "libs/schematics/package.json",
            "main": "libs/schematics/src/index.ts",
            "assets": [
              "libs/schematics/*.md",
              {
                "glob": "**/*.json",
                "input": "libs/schematics/src/lib",
                "output": "./lib/"
              },
              {
                "glob": "**/*.*__tmpl__",
                "input": "libs/schematics/src/lib",
                "output": "./lib/"
              },
              {
                "glob": "**/files/{*,.*}",
                "input": "libs/schematics/src/lib",
                "output": "./lib/"
              }
            ]
          }
        }
      }
    },
    "nest": {
      "root": "libs/nest",
      "sourceRoot": "libs/nest/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/nest/.eslintrc",
            "tsConfig": ["libs/nest/tsconfig.lib.json", "libs/nest/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/nest/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/nest/jest.config.js",
            "tsConfig": "libs/nest/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/nest",
            "tsConfig": "libs/nest/tsconfig.lib.json",
            "packageJson": "libs/nest/package.json",
            "main": "libs/nest/src/index.ts",
            "assets": [
              "libs/nest/*.md",
              {
                "input": "./libs/nest/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./libs/nest",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./libs/nest",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "nest-e2e": {
      "projectType": "application",
      "root": "apps/nest-e2e",
      "sourceRoot": "apps/nest-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nest:build",
            "npmPackageName": "@nxpm/nest",
            "pluginOutputPath": "dist/libs/nest",
            "jestConfig": "apps/nest-e2e/jest.config.js",
            "tsSpecConfig": "apps/nest-e2e/tsconfig.spec.json"
          }
        }
      }
    },
    "react-native": {
      "root": "libs/react-native",
      "sourceRoot": "libs/react-native/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/react-native/.eslintrc",
            "tsConfig": [
              "libs/react-native/tsconfig.lib.json",
              "libs/react-native/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/react-native/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/react-native/jest.config.js",
            "tsConfig": "libs/react-native/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/react-native",
            "tsConfig": "libs/react-native/tsconfig.lib.json",
            "packageJson": "libs/react-native/package.json",
            "main": "libs/react-native/src/index.ts",
            "assets": [
              "libs/react-native/*.md",
              {
                "input": "./libs/react-native/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./libs/react-native",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./libs/react-native",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "react-native-e2e": {
      "projectType": "application",
      "root": "apps/react-native-e2e",
      "sourceRoot": "apps/react-native-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "react-native:build",
            "npmPackageName": "@nxpm/react-native",
            "pluginOutputPath": "dist/libs/react-native",
            "jestConfig": "apps/react-native-e2e/jest.config.js",
            "tsSpecConfig": "apps/react-native-e2e/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  }
}
